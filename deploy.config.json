{
  "apps": [
    {
      "name": "crawler",
      "script": "./src/worker/crawler",
      "env": {
        "NODE_PATH": "src"
      },
      "instances": 2,
      "exec_mode": "cluster",
      "merge_logs": true,
      "log_date_format": "YYYY-MM-DD HH:mm::ss.SSS Z"
    },
    {
      "name": "observer",
      "script": "./src/worker/observer",
      "env": {
        "NODE_PATH": "src"
      },
      "instances": 2,
      "exec_mode": "cluster",
      "merge_logs": true,
      "log_date_format": "YYYY-MM-DD HH:mm::ss.SSS Z"
    }
  ]
}